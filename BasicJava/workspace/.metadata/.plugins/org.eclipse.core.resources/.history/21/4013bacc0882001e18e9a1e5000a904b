package dao;

import java.util.List;

import util.JDBCUtil;

public class BookDao {
	private static BookDao singleTon = null;

	private BookDao() {
	};

	public static BookDao getInstance() {
		if (singleTon == null) {
			singleTon = new BookDao();
		}
		return singleTon;
	}
	
	JDBCUtil jdbc = JDBCUtil.getInstance();
	
}
