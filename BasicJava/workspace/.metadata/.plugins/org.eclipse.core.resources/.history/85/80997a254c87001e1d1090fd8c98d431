	public boolean chkDate(String str) {

		try {
	         SimpleDateFormat  sdf = new  SimpleDateFormat("yy-MM-dd");
	         sdf.setLenient(false);
	          sdf.parse(str);
	          return true;
		} catch (ParseException  e){
			return false;
		}
		
	}

