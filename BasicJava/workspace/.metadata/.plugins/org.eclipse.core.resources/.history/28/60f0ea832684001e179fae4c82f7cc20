package print;

public class MemberPrint {
	
	public void printVar() {
		System.out.println("------------------------------------");
	}
	
	public void printLn(int num) {
		for(int i=0; i<num; i++) System.out.println();
	}
	
	public void memberHome() {
		printVar();
		System.out.println("1. 도서대출");
		System.out.println("2. 내 대출 목록");
		System.out.println("3. 열람실 예약");
		System.out.println("4. 문화행사프로그램 예약");
		System.out.println("5. 정보수정");
		printVar();
	}
	
	public void printMBHome() {
		printVar();
		System.out.println("로그인해주세요.");

	}

	public void  printMemberHome() {
		printVar();
		System.out.println("1.회원 로그인");
		System.out.println("2.회원 가입");
	}
}
